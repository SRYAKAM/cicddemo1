#%RAML 1.0 DataType
type: object
properties:
  accountId: string
  accountType:
    enum: [Checking, Savings, Overdraft Savings, Credit Card]
  accountNumber: string
  accountOwner:
    type: array
    items: AccountOwner
  accountBalance: Money
  PAN:
    type: string
    pattern: ^[A-Z]{5,5}[0-9]{4,4}[A-Z]{1}
  bank: Bank
  intrestRate?:
    type: number
    format: double
  createdAt: datetime
  modifiedAt?: datetime
example: !include../examples/AccountExample.raml



  
